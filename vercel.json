{
  "builds": [
    {
      "src": "djangoProject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "./public/staticfiles/$1"
    },
    {
      "src": "(.*)",
      "dest": "djangoProject/wsgi.py"
    }
  ]
}
